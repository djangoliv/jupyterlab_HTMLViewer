    "@jupyterlab/notebook": "^0.18.4",
    "@phosphor/disposable": "^1.1.2",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.7"
  },
  "devDependencies": {
    "rimraf": "^2.6.1",
    "typescript": "~2.9.2"
